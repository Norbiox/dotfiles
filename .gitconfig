[user]
	name = Norbiox
	email = norbertchmiel.it@gmail.com
[core]
	editor = vim
	pager = less
	excludesFile = /home/norbert/.gitignore
[url "git@github.com:"]
	insetadOf = https://github.com/
[diff]
	tool = vimdiff
[difftool]
	prompt = false

[includeIf "gitdir:~/Work/Onwelo/"]
	path = ~/Work/Onwelo/.gitconfig
[safe]
	directory = /opt/flutter
	directory = /home/norbert/Work/Onwelo/repos/ce-ocr/
	directory = /home/norbert/Work/Onwelo/repos/ce-ocr
[push]
	autoSetupRemote = true
	followTags = true
	default = upstream
[alias]
	root = rev-parse --show-toplevel
[merge]
	tool = vimdiff
[rerere]
	enabled = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
